#include<iostream>
#include<string.h>
using namespace std;


void contain()
{   cout<<"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~"<<endl;
    cout<<"Password must contain at least one uppercase letter (A-Z), one lowercase letter (a-z),"<<endl;
    cout<< "one number (0-9), and one special character (like !, @, #, $, %, ^, &, *, etc.)"<<endl;
    cout<<"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~"<<endl;
}


int main()
{
    contain();
    
    char c[]="ashwani_bish29@";
    

    
    for(int i=0;i<strlen(c);i++)
    {    
        if(c[i]>=48&&c[i]<=57)
        {
            cout<<"contain digits,";
            
            
            
        }
        else
        {
            cout<<"not contain digits,";
        }
        
        
        if(c[i]>=65&&c[i]<=90)
        {
            cout<<"contain uppercase  character,";
            break;
        }
        else
        {
            cout<<"not contain uppercase  character,";
        }
        
        
        if(c[i]>=97&&c[i]<=122)
        {
            cout<<"contain lowercase character,";
                break;}
        else
        {
            cout<<"not contain uppercase  character,";
        }
        
        
        
         if(c[i]>=33&&c[i]<=47)
        {
            cout<<"contain special character,";
            break;
        }
        else
        {
            cout<<"not contain special character,";
        }
    }

    }
